{
  "manifest_version": 2,
  "name": "Fast QR",
  "version": "1.0.2",
  "description": "QR your currently viewed page. This plugin doesn't require network access",
  "icons": {
    "128": "128.png",
    "16": "16.png",
    "48": "48.png"
  },
  "browser_action": {
    "default_icon": "19.png",
    "default_popup": "popup.html"
  },
  "permissions": [ "tabs" ]
}
